{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "BucketAccess",
            "Action": [
                "s3:Get*",
                "s3:Head*",
                "s3:List*",
                "s3:Put*",
                "s3:DeleteObject"
            ],
            "Effect": "Allow",
            "Resource": [
                "arn:aws:s3:::__BUCKET_NAME_HERE__",
                "arn:aws:s3:::__BUCKET_NAME_HERE__/*"
            ]
        },
        {
            "Sid": "KMSAccess",
            "Action": [
                "kms:Decrypt",
                "kms:DescribeKey",
                "kms:Encrypt",
                "kms:GenerateDataKey*",
                "kms:ReEncrypt*"
            ],
            "Effect": "Allow",
            "Resource": "arn:aws:kms:us-east-1:XXXXXXXXXXXX:key/XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX"
        },
        {
            "Sid": "SimpleEmailServiceAccess",
            "Action": [
                "ses:GetSendQuota",
                "ses:SendEmail",
                "ses:SendRawEmail"
            ],
            "Effect": "Allow",
            "Resource": "*"
        },
        {
            "Sid": "CloudWatchMetricUpdateAccess",
            "Action": [
                "cloudwatch:PutMetricData",
                "cloudwatch:GetMetricStatistics",
                "cloudwatch:ListMetrics",
                "ec2:DescribeTags"
            ],
            "Effect": "Allow",
            "Resource": "*"
        },
        {
            "Sid": "LamdaInvokeFunctionAccess",
            "Effect": "Allow",
            "Action": "lambda:InvokeFunction",
            "Resource": "arn:aws:lambda:us-east-1:519887312542:function:deep-util-services-XXXXXXX-sourceExtract"
        },
        {
            "Sid": "ElasticSearchAccess",
            "Action": [
                "es:*"
            ],
            "Effect": "Allow",
            "Resource": "*"
        },
        {
            "Sid": "DynamodbAcess",
            "Action": [
                "dynamodb:Query",
                "dynamodb:Scan",
                "dynamodb:GetItem",
                "dynamodb:DescribeTable"
            ],
            "Resource": [
                "arn:aws:dynamodb:us-east-1:*:table/deep-util-services-XXXXXXX-sources"
            ],
            "Effect": "Allow"
        }
    ]
}
