# vim: set ft=yaml:

option_settings:
  aws:elasticbeanstalk:application:environment:
    # Generated from deploy/deploy_deeper.sh (deploy/scripts/travis_deploy.sh)
    DEEP_ENVIRONMENT: ${DEEP_ENVIRONMENT}
    EBS_ENV_TYPE: ${EBS_ENV_TYPE}
    DEPLOYMENT_ENV_NAME: ${DEPLOYMENT_ENV_NAME}
    # S3 Storage
    DJANGO_USE_S3: ${DJANGO_USE_S3}
    DJANGO_AWS_STORAGE_BUCKET_NAME_MEDIA: ${DJANGO_AWS_STORAGE_BUCKET_NAME_MEDIA}
    DJANGO_AWS_STORAGE_BUCKET_NAME_STATIC: ${DJANGO_AWS_STORAGE_BUCKET_NAME_STATIC}
    # Papertrail
    USE_PAPERTRAIL: ${USE_PAPERTRAIL}
    PAPERTRAIL_HOST: ${PAPERTRAIL_HOST}
    PAPERTRAIL_PORT: ${PAPERTRAIL_PORT}
    # Google recaptcha
    RECAPTCHA_SECRET: ${RECAPTCHA_SECRET}
    HCAPTCHA_SECRET: ${HCAPTCHA_SECRET}
    # Django
    DJANGO_SECRET_KEY: ${DJANGO_SECRET_KEY}
    DJANGO_DEBUG: False
    NO_DJANGO_MIGRATION: True
    DJANGO_ALLOWED_HOST: ${DJANGO_ALLOWED_HOST}
    DJANGO_CACHE_REDIS_URL: ${DJANGO_CACHE_REDIS_URL}
    FRONTEND_HOST: ${FRONTEND_HOST}
    # DEEP
    DEEPER_SITE_NAME: ${DEEPER_SITE_NAME}
    DEEP_HTTPS: ${DEEP_HTTPS}
    # HID
    HID_CLIENT_ID: ${HID_CLIENT_ID}
    HID_CLIENT_REDIRECT_URL: ${HID_CLIENT_REDIRECT_URL}
    HID_AUTH_URI: ${HID_AUTH_URI}
    # DB
    DATABASE_NAME: ${DATABASE_NAME}
    DATABASE_USER: ${DATABASE_USER}
    DATABASE_PASSWORD: ${DATABASE_PASSWORD}
    DATABASE_HOST: ${DATABASE_HOST}
    DATABASE_PORT: ${DATABASE_PORT}
    # Email
    USE_EMAIL_CONFIG: ${USE_EMAIL_CONFIG}
    EMAIL_HOST_USER: ${EMAIL_HOST_USER}
    # Celery
    CELERY_REDIS_URL: ${CELERY_REDIS_URL}
    CHANNEL_REDIS_URL: ${CHANNEL_REDIS_URL}
    FLOWER_BASIC_AUTHS: ${FLOWER_BASIC_AUTHS}
    # Sentry
    SENTRY_DSN: ${SENTRY_DSN}
    # NOT REQUIRED (Remove this later)
    S3_AWS_ACCESS_KEY_ID:
    S3_AWS_SECRET_ACCESS_KEY:
    AWS_ACCESS_KEY_ID:
    AWS_SECRET_ACCESS_KEY:
    DJANGO_TIME_ZONE:
    DJANGO_ALLOWED_HOST_API:
    DJANGO_ALLOWED_HOST_WEBSOCKET:
    SES_AWS_ACCESS_KEY_ID:
    SES_AWS_SECRET_ACCESS_KEY:
    EMAIL_HOST:
    EMAIL_HOST_PASSWORD:
    EMAIL_PORT:
